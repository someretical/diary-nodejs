{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": ["@typescript-eslint", "prettier"],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended",
		"prettier"
	],
	"env": {
		"es6": true,
		"node": true
	},
	"ignorePatterns": ["*.js"],
	"rules": {
		"no-console": 1,
		"prettier/prettier": 2,
		"sort-imports": 1
	}
}
